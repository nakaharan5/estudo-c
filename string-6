#include <stdio.h>
#include <string.h>

int main(){
	char estado[2];
	
	printf("Digite as siglas dos estados do sul: ");
	gets(estado);
	
	if(estado[0]=='P' && estado[1]=='R'){
		printf("Paran√°");
		
	} else if (estado[0]=='R' && estado[1]=='S'){
		printf("Rio Grande do Sul");
	} else if (estado[0]=='S' && estado[1]=='C'){
		printf("Santa Catarina");
	} else{
		printf("Nao e estado da regiao sul");
	}
	
	return(0);
}
